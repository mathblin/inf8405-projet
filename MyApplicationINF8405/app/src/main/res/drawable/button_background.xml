<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <corners android:radius="20dp" />
    <solid android:color="@color/green" />
    <padding
        android:left="16dp"
        android:top="8dp"
        android:right="16dp"
        android:bottom="8dp" />
    <size
        android:width="48dp"
        android:height="48dp" />

    <stroke
        android:width="3dp"
        android:color="#0048FF" />

    <rotate
        android:fromDegrees="45"
        android:toDegrees="45"
        android:pivotX="100%"
        android:pivotY="100%">
        <shape android:shape="line">
            <stroke android:color="#D62525" android:width="3dp" />
        </shape>
    </rotate>

    <rotate
        android:fromDegrees="-45"
        android:toDegrees="-45"
        android:pivotX="100%"
        android:pivotY="0%">
        <shape android:shape="line">
            <stroke android:color="#FFFFFF" android:width="3dp" />
        </shape>
    </rotate>
</shape>






